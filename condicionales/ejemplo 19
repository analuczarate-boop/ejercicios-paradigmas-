n1=int (input("Ingrese el primer numero: "))
n2=int (input("Ingrese el segundo numero: "))
n3=int (input("Ingrese el tercer numero: "))
if n1>n2 and n1>n3: 
    print("El numero mas grande es el primer numero: ",n1)
else:
    if n2>n3:
      print("El numero mas grande es el segundo numero: ",n2)
    else: 
      if n3>n1 and n3>n2:
        print("El numero mas grande es el tercer numero: ",n3) 
      else: print("Los numeros son iguales")